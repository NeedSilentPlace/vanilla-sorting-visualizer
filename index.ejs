<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width">
    <title><%= htmlWebpackPlugin.options.title %></title>
</head>
<body>
  <div class="container">
    <div class="title">
        <h1>SORTING HOUSE</h1>
    </div>
    <div class="input">
      <input id="box"type="text" placeholder="give me a number!">
    </div>
    <div id="type">
      <div class="bubble">Bubble</div>
      <div class="insertion">Insertion</div>
      <div class="merge">Merge</div>
      <div class="selection">Selection</div>
      <div class="clear">Clear</div>
    </div>
    <div class="run">
      <div id="btn">Sorting!</div>
    </div>
    <div class="hoisting">
      <span id="standard">Room</span>
    </div>
    <div id="show">

    </div>
  </div>
</body>
</html>
